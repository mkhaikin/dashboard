<form >
        <div class="form-group" >
        <input type="hidden" name="condoName" value="{{data.0.name}}">
        <input type="hidden" name="noticeID" value="{{id}}">
        <div class="iconlist" style="display:inline;">
            {{#each data}}
            {{#if this.picid}}
                {{#compare this.picid "==" 1 }}
                    <div class="pic" style="display:inline;">
                {{else}}
                    <div class="pic" style="display:none;">
                {{/compare}}        
                    <img src="{{this.icon}}" id="addpic{{this.picid}} " >   
                    </div>                
            {{/if}}
            {{/each}}
        </div>
        <div class="notice-group" style="display:inline;">
            <input type="text" required placeholder="Add notice " class="form-control" name="text">
            <!--<textarea id="editor" cols="50" rows="10" required ></textarea>-->
            <div id="editor"></div>
            <input type="text" placeholder="Put Start date here" class="date" id="date_pickerStart" name="start" readonly />
            <input type="text" placeholder="Put End date here" class="date" id="date_pickerEnd" name="end" readonly/>
        </div>
        <div class="addbutton-group" style="display:inline;" >
            <button class="btn btn-success" type="submit" id="data_submit" name="addNotice" disabled = "true">Add</button>
        </div>
        <div class="editbutton-group" style="display:none;">
            <button class="btn btn-success" type="button" name="editNotice">Save</button>            
        </div>            
            <button class="btn btn-success" type="button" id="data_preview" name="previewNotice" disabled = "true">Preview</button>
            <button class="btn btn-success" type="button" name="cancelEdit">Cancel</button>
        </div>
    </div>
</form>