<form action="POST">
    <input type="hidden" name="condoName" value="">
    <div class="notice-group form-group">
        <input type="text" required placeholder="Add notice " class="form-control" name="text">
        <div id="editor"></div>
        <textarea class="form-control" name="textBody" id="" cols="30" rows="10"></textarea>
        <input type="text" placeholder="Select Start date" class="date" id="date_pickerStart" name="start" readonly />
        <input type="text" placeholder="Select End date" class="date" id="date_pickerEnd" name="end" readonly/>
    </div>
        <div class="addbutton-group" style="display:inline;" >
        <button class="btn btn-success" type="submit" id="data_submit" name="addNotice" disabled = "true">Add</button>
    </div> 
</form>